{
  "compilerOptions": {
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "pop": ["src"],
      "pop/*": ["src/*", "*"]
    }
  },
  "include": [".dumirc.ts", "src/**/*"],
  "exclude": ["dist", "docs", "docs-dist"]
}
